    <script src="assets/admin/global/vendor/breakpoints/breakpoints.js"></script>
    <script>
    Breakpoints();
    </script>
</head>
<body class="animsition">
    
    {# Menú de arriba #}
    {% include 'overall/topnav' %}
    
    {# Barra Lateral #}
    {% include 'overall/menu' %}
    
