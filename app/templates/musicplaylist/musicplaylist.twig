{% extends 'overall/layout' %}
{% block page_title %}Music And Playlist{% endblock %}

{% block appBody %}

{# Header #}
{% include 'overall/topnav' with {page_title: 'Music & Playlist' } only %}

<div class="main-header">
	<div class="content-bg-wrap bg-music"></div>
	<div class="container">
		<div class="row">
			<div class="col col-lg-8 m-auto col-md-8 col-sm-12 col-12">
				<div class="main-header-content">
					<h1>Listen and Share Music!</h1>
					<p>Here you’ll be able to manage your Spotify playlist widget and listen all the saved playlists from your
	 friends! Friends playlists will update when they update their Olympus playlists, that way you can build awesome playlists
	  to share with your friends every day and they will always be updated.</p>
				</div>
			</div>
		</div>
	</div>

	<img class="img-bottom" src="assets/dragsport/img/music-bottom.png" alt="friends">
</div>

<!-- ... end Main Header Music -->

<div class="container">
	<div class="row">
		<div class="col col-xl-9 order-xl-2 col-lg-6 order-lg-2 col-md-12 order-md-1 col-sm-12 col-12">
			<div class="ui-block">
				<div class="ui-block-title">
					<h6 class="title">Your Playlist Widget Settings</h6>
				</div>
				<div class="ui-block-content">


					
					<!-- Music Playlist Form  -->
					
					<form>
					
						<fieldset class="form-group label-floating is-select">
							<label class="control-label">Songs Showed Before the Jump</label>
							<select class="selectpicker form-control">
								<option value="AL">Show 5 Songs</option>
								<option value="2">Show 7 Songs</option>
								<option value="WY">Show 10 Songs</option>
							</select>
						</fieldset>
					
						<fieldset class="form-group label-floating is-select">
							<label class="control-label">Album Cover Options</label>
							<select class="selectpicker form-control">
								<option value="AL">Show Album Covers</option>
								<option value="2">Show Album Covers</option>
								<option value="WY">Show Album Covers</option>
							</select>
						</fieldset>
					
					</form>
					
					{% if false != user %}
					<p class="mt50">You are logged in to Spotify as <a href="{{ user.external_urls.spotify }}" target="_blank" class="c-green"> {{ user.display_name }}</a></p>
					{% endif %}	

					<div class="row">
						<div class="col col-xl-6 col-lg-12 col-md-12 col-sm-12 col-12">
							<a href="javascript:void(0)" class="btn btn-secondary btn-lg full-width" data-toggle="modal" data-target="#faqs-popup">Link your Playlist FAQs</a>
						</div>
						<div class="col col-xl-6 col-lg-12 col-md-12 col-sm-12 col-12">
							{% if false == user %}
							<a href="{{ url }}" class="btn btn-green btn-lg full-width btn-icon-left"><i class="fab fa-spotify" aria-hidden="true"></i> Spotify Account Login</a>
							{% else %}
							<a href="musicplaylist/logout" class="btn btn-green btn-lg full-width btn-icon-left"><i class="fab fa-spotify" aria-hidden="true"></i>
								Spotify Account Logout</a>
							{% endif %}

						</div>
					</div>
				</div>
			</div>

		</div>
		{% for i,playlist in user.pl if false != user.pl %}
		<div class="col col-xl-3 {{ i == 0 ? 'order-xl-1 order-lg-1 order-md-2' : 'order-xl-2 order-lg-2 order-md-2' }} col-lg-6  col-md-12 col-sm-12 col-12">
			<div class="ui-block">
				<div class="ui-block-title">
					<h6 class="title">{{ playlist.name }}</h6>
					<a href="{{ playlist.external_urls.spotify }}" class="more" target="_blank">
						<svg class="olymp-three-dots-icon"><use xlink:href="assets/dragsport/svg-icons/sprites/icons.svg#olymp-three-dots-icon"></use></svg>
					</a>
				</div>
				
				<ol class="widget w-playlist playlist-spf-dg">
					{% for track in playlist.data_tracks.items %}

					<li class="js-open-popup" data-popup-target=".playlist-popup">
						<div class="playlist-thumb">
							<img src="{{ track.track.album.images[2]['url'] }}" alt="thumb-composition">
							<div class="overlay"></div>
							<a href="javascript:void(0)" class="play-icon">
								<svg class="olymp-music-play-icon-big">
									<use xlink:href="assets/dragsport/svg-icons/sprites/icons-music.svg#olymp-music-play-icon-big"></use>
								</svg>
							</a>
						</div>
				
						<div class="composition">
							<a href="javascript:void(0)" class="composition-name">{{ track.track.name }}</a>
							<a href="javascript:void(0)" class="composition-author">{{ track.track.artists[0].name }}</a>
						</div>
				
						<div class="composition-time">
							<time class="published" datetime="{{ msToTime(track.track.duration_ms) }}">{{ msToTime(track.track.duration_ms) }}</time>
							{#<div class="more">
								<svg class="olymp-three-dots-icon">
									<use xlink:href="assets/dragsport/svg-icons/sprites/icons.svg#olymp-three-dots-icon"></use>
								</svg>
								<ul class="more-dropdown">
									<li>
										<a href="javascript:void(0)">Add Song to Player</a>
									</li>
									<li>
										<a href="javascript:void(0)">Add Playlist to Player</a>
									</li>
								</ul>
							</div>#}
						</div>

				
					</li>
					{% endfor %}


				</ol>

			</div>
		</div>
		{% endfor %}

	</div>
</div>

